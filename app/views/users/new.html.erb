<div class="container-fluid">
  <div class="row header">
    <div id="cos-sticker"class="col-lg-12">
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
    </div>
  </div>
  <div class="row">
    <div class="col-md-5">
      <nav id="nav" class="navbar navbar-default">
        <div class="container-fluid">
          <ul class="nav nav-pills red">
            <li id="music-link" role="presentation"><a href="#">Music</a></li>
            <li id="shows-link" role="presentation"><a href="#">Shows</a></li>
            <li id="gallery-link" role="presentation"><a href="#">Gallery</a></li>
            <li id="contact-link" role="presentation"><a href="#">Contact</a></li>
          </ul>
        </div>
      </nav>
    </div>
  </div>
  <div class="row welcome">
    <div class="col-sm-5 col-sm-offset-1 latest">
      <h1>The Latest</h1>
      <p>Check out our show at <a href="#shows-section">Quencher's</a> April 8!</p>
      <p>New <a href="#music-section">music</a> coming soon.</p>
      <iframe width="70%" height="60" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/224234806&amp;color=ff5500&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false"></iframe>
    </div>
    <div class="col-sm-3 col-sm-offset-2 email">
      <p>Join our mailing list</p>
      <span id="email_message"></span>
      <%= form_for(@user, remote: true) do |f| %>


        <%= f.label :email %>
        <%= f.email_field :email %>

        <%= f.submit 'Sign Up', class: 'btn btn-danger btn-sm'%>
      <% end %>
    </div>
  </div>

  <div id="music-section" class="row music">
    <div class="row soundcloud">
      <div class="col-sm-4 col-sm-offset-1">
        <iframe width="100%" height="100" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/224234846&amp;color=ff5500&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false"></iframe>
      </div>
      <div class="col-sm-4 col-sm-offset-1">
        <iframe width="100%" height="100" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/224234806&amp;color=ff5500&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false"></iframe>
      </div>
    </div>
    <div class="col-sm-4 col-sm-offset-2">
      <div class="row youtube">
          <iframe width="550" height="315" src="https://www.youtube.com/embed/nvAD12G6-B4" frameborder="0" allowfullscreen></iframe>
      </div>
    </div>
  </div>
  <div id="shows-section" class="row shows">
    <%= render "/shows/index" %>
  </div>
  <div class="fake-footer">
    <div class="foot-sticker">
      <div class="row">
        <div class="col-xs-12 img-responsive">
          <img src="http://i.imgur.com/pwaJbcJ.jpg" alt="mason cos horizontal" />
        </div>
      </div>
    </div>
  </div>
  <div id="gallery-section" class="row gallery">
    <div id="image-carousel" class="carousel slide col-sm-6 col-sm-offset-3" data-ride="carousel">
      <ol class="carousel-indicators">
        <li data-target="#image-carousel" data-slide-to="0" class="active"></li>
        <li data-target="#image-carousel" data-slide-to="1"></li>
        <li data-target="#image-carousel" data-slide-to="2"></li>
        <li data-target="#image-carousel" data-slide-to="3"></li>
        <li data-target="#image-carousel" data-slide-to="4"></li>
        <li data-target="#image-carousel" data-slide-to="5"></li>
      </ol>
      <div class="carousel-inner" role="listbox">
        <div class="item active">
          <%= image_tag 'Mason_Cos_Live_Pic.jpg', class: 'img-responsive center-block' %>
        </div>
        <div class="item">
          <%= image_tag 'Group_Mason_Cos.jpg', class: 'img-responsive center-block' %>
        </div>
        <div class="item">
          <%= image_tag 'Sam_Sadie_Standing.jpg', class: 'img-responsive center-block' %>
        </div>
        <div class="item">
          <%= image_tag 'TJ_Standing_at_Comps.jpg', class: 'img-responsive center-block' %>
        </div>
        <div class="item">
          <%= image_tag 'Shadow_of_Mic.jpg', class: 'img-responsive center-block' %>
        </div>
        <div class="item">
          <%= image_tag 'Pic_of_Drums.jpg', class: 'img-responsive center-block' %>
        </div>
      </div>
      <a class="left carousel-control" href="#image-carousel" role="button" data-slide="prev">
        <span class="icon-prev" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#image-carousel" role="button" data-slide="next">
        <span class="icon-next" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </div>
  <hr>
  <div id="contact-section" class="row contact">
    <div class="row">
      <div class="col-sm-4 col-sm-offset-4 name">
        <h2>Mason Cos</h2>
        <p>
          Chicago, IL
        </p>
        <h3>
          <a href="mailto:masoncosband@gmail.com">
            masoncosband@gmail.com
          </a>
        </h3>
      </div>
    </div>
    <div class="row logos">
      <div class="col-sm-1 col-sm-offset-4">
        <a target="_blank" href="https://www.facebook.com/MasonCosMusic"><%= image_tag 'FB-f-Logo__white_50.png' %></a>
      </div>
      <div class="col-sm-2">
        <a target="_blank" href="https://soundcloud.com/mason-cos"><%= image_tag 'sc_sbs_white_192x24.png' %></a>
      </div>
      <div class="col-sm-1">
        <a target="_blank" href="https://github.com/uelski/mason_test"><%= image_tag 'GitHub-Mark-64px.png' %></a>
      </div>
    </div>
  </div>

</div>
